{
  "name": "koyo-job-admin-frontend",
  "type": "module",
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "scripts": {
    "build": "NODE_ENV=production node build.mjs",
    "lint": "tsc -skipLibCheck -noEmit -p .",
    "watch": "node build.mjs"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.2.3",
    "@fontsource/outfit": "^5.1.0",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-router": "^7.0.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "esbuild": "^0.24.1",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-sass-plugin": "^3.3.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "prettier": {
    "importOrder": [
      "^[@a-zA-Z0-9_-]",
      "^(.?./)"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ]
  }
}
